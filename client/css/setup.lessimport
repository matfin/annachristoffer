html {
	width: 100%;
	height: 100%;
	-webkit-font-smoothing: antialiased;
}

body {
	width: 100%;
	height: 100%;
	font-family: @standardFontFamily;
	font-size: @baseFontSize;
}

.container {
	width: 100%;
	height: 100%;
	text-align: center;
}

.wrapper {
	margin: 0 auto;
	width: 100%;
	height: 100%;
	max-width: @maxWidth;
	background-color: lightblue;
}