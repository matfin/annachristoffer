body.page {
	.wrapper {
		height: 100%;
		.backgroundImage('/images/backgrounds/cv');
		background-repeat: no-repeat;
		background-size: cover;
		padding-bottom: 75%;
		.container {
			.content {
				text-align: center;
				> section {
					margin: 0 auto;
					text-align: left;
					max-width: @gridColumnWidth * 52;
					padding: @gridColumnWidth * 4 0;
				}
			}
		}
	}
}