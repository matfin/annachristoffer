body.list {
	.backgroundImage('/images/backgrounds/landing');
	.wrapper {
		.container {	
			.header {
				padding-top: @gridColumnWidth * 4;
			}
			.content {
				padding-top: @gridColumnWidth * 4;
				section {
					@media @mobile {
						padding-top: 0px;
						padding-bottom: @gridColumnWidth * 4;
					}
				}
			}
		}
	}
}

ul.projectGroup {
	display: inline;
	float: left;
}