body.list {
	.wrapper {
		.container {	
			.content {
				> section {
					padding: @gridColumnWidth * 4 0;

					@media @tablet {
						padding: @gridColumnWidth 0;
					}
				}
			}
			.content-mobile {
				> section {
					padding: @gridColumnWidth * 3 0;
				}
			}
		}
	}
}

ul.projectGroup {
	display: inline;
	float: left;

	li {
		display: block;
		width: 100%;
		float: left;
		.box-sizing(border-box);
		padding: @gridColumnWidth / 2;
	}
}