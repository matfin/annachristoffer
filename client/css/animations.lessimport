/**
 *	TV Flicker animation
 */
@-webkit-keyframes badtv {

	25% {
		.filter(saturate(10));
	}

	50% {
		.filter(saturate(0));
		.filter(hue-rotate(90deg));
	}

	75% {
		.filter(hue-rotate(0deg));
		.filter(invert(100%));
	}

	100% {
		.filter(invert(0%));
	}
}

@-moz-keyframes badtv {
}

@-o-keyframes badtv {
}

